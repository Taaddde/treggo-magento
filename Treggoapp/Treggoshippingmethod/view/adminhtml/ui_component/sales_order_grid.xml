<?xml version="1.0" encoding="UTF-8"?>

<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="treggoordersprinta4labels">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">treggoordersprinta4labels</item>
                        <item name="label" xsi:type="string" translate="true">Imprimir etiquetas A4</item>
                        <item name="url" xsi:type="url" path="treggoprintlabel/labels/printa4inbulk"/>
                    </item>
                </argument>
            </action>
            <action name="treggoordersprintzebralabels">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">treggoordersprintzebralabels</item>
                        <item name="label" xsi:type="string" translate="true">Imprimir etiquetas Zebra</item>
                        <item name="url" xsi:type="url" path="treggoprintlabel/labels/printzebrainbulk"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>